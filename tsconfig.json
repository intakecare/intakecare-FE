{
  "compilerOptions": {
    "target": "esnext",
    "module": "esnext",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "jsx": "preserve",
    "sourceMap": true,
    "skipLibCheck": true,
    "moduleResolution": "node",
    "noImplicitAny": false,
    "resolveJsonModule": true,
    "types": ["webpack-env","vite/client", ],
    "paths": {
        "@/*": ["./src/*"]
    }
  }
}
