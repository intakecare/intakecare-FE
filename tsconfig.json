{
  "compilerOptions": {
    "target": "esnext",
    "module": "esnext",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true,
    "moduleResolution": "node",
    "noImplicitAny": false,
    "resolveJsonModule": true,
    "types": ["vite/client"],
    "paths": {
        "@/*": ["./src/*"]
    }
  }
}
